module blinker (
    input clk,  // clock
    input rst,  // reset
    output blink, // output to LED
    output hold_time[27]
  ) {

  dff counter[26](.clk(clk), .rst(rst)); // for LED blink
  dff holder[27](.clk(clk), .rst(rst));  // for time of holding
  
  always {
    blink = counter.q[24];
    counter.d = counter.q + 1;
    hold_time = holder.q[26:0];
    holder.d = holder.q + 1;
  }
}
